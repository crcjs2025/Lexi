body { counter-reset: example 0 figure 0 issue 0; }

.issue { counter-increment: issue 1; }

.issue:not(.no-marker)::before { content: "Issue " counter(issue); }

.example { counter-increment: example 1; }

.example:not(.no-marker)::before { content: "Example " counter(example); }

.invalid.example:not(.no-marker)::before, .illegal.example:not(.no-marker)::before { content: "Invalid Example" counter(example); }

figcaption { counter-increment: figure 1; }

figcaption:not(.no-marker)::before { content: "Figure " counter(figure) " "; }
