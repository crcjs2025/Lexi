.note > p:first-child, .issue > p:first-child, blockquote > :first-child, .amendment > p:first-child, .correction > p:first-child, .addition > p:first-child { margin-top: 0px; }

.note > p:last-child, .issue > p:last-child, blockquote > :last-child, .amendment > p:last-child, .correction > p:last-child, .addition > p:last-child { margin-bottom: 0px; }

